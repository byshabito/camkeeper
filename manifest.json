{
  "name": "CamKeeper",
  "author": "Shabito",
  "description": "Local webcam bookmarker for Chaturbate & Stripchat",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab"],
  "action": {
