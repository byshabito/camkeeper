{
  "name": "CamKeeper",
  "author": "Shabito",
  "description": "Cross-site bookmark manager for Chaturbate and Stripchat models",
  "version": "0.4.1",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "contextMenus", "tabs"],
  "host_permissions": ["https://chaturbate.com/*", "https://stripchat.com/*"],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "action": {
    "default_title": "CamKeeper",
    "default_popup": "src/entries/popup/index.html"
  },
  "background": {
    "service_worker": "src/entries/background.js"
  },
  "options_ui": {
    "page": "src/entries/options/index.html",
    "open_in_tab": true
  },
  "commands": {
    "open-library": {
      "suggested_key": {
        "default": "Alt+Shift+L"
      },
      "description": "Open CamKeeper Library"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "camkeeper@shabito.net",
      "strict_min_version": "109.0"
    }
  }
}
